{
  "name": "remix-infonomic-io-c6b7",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install .husky",
    "build": "run-s build:*",
    "build:css": "postcss shared/css/tailwind.css shared/css/app.css app/routes/**/*.css app/modules/**/*.css --env production --dir app/styles --base ./",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "postcss shared/css/tailwind.css shared/css/app.css app/routes/**/*.css app/modules/**/*.css --env development --dir app/styles --base ./ --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "debug:remix": "cross-env NODE_ENV=development binode --inspect-brk --require ./mocks -- @remix-run/dev:remix dev",
    "debug": "run-p dev:css debug:remix",
    "format": "prettier --config ./prettier.config.js --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext js,jsx,ts,tsx .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "cross-env PORT=3000 playwright test --project=chromium",
    "test:e2e:install": "npx playwright install chromium --with-deps",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.8.0",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-icons": "^1.1.1",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-primitive": "^1.0.1",
    "@radix-ui/react-select": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-toast": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.0.2",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@tanstack/react-table": "^8.7.4",
    "@types/react-transition-group": "^4.4.5",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "isbot": "^3.5.3",
