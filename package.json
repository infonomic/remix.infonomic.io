    "vitest": "^0.26.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "browserslist": [
    "> 2%"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
