    "material-ripple-effects": "^2.0.1",
    "prisma": "^4.8.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-spinners": "^0.13.6",
    "sass": "^1.55.0",
    "tailwind-merge": "^1.8.0",
    "tailwindcss-radix": "^2.6.1",
